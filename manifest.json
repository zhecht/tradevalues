{
	"name": "Trade Value Calculator",
	"version": "0.0.3.3",
	"description": "Interactive trade value calculator with values taken from /u/PeakedInHighSkool",
	"permissions": ["<all_urls>", "activeTab", "declarativeContent", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"48": "images/trade_48.png"
		}
	},
	"icons": {
		"48": "images/trade_48.png"
	},
	"manifest_version": 2
}
